{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react-native",
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "typeRoots": ["@types"]
    },
    "typedocOptions": {
        "entryPoints": [
            "App.tsx",
            "types.tsx",
            "screens",
            "redux",
            "navigation",
            "models",
            "hooks",
            "constants",
            "components",
            "@types"
        ],
        "out": "docs"
    }
}
